.header{
	font-family:'NanumSquareB';
	color:#4f2ca5;
	font-size:1.5em;
	font-weight:bold;
	text-align:center;
	width:100%;
	height:50px;
	border: 1px solid;
	padding-top:12px;
	background: #008ae8;
	border-color:#ffffff; 
	border-bottom-color: #dddddd;
}
.header .left_btn {
	color: #000000;
	position:absolute;
	left:10px;
}
.header .left_btn img {
	width:30px;
	height:30px;
}
.header .title {
	width:100%;
	line-height:50px;
	position:absolute;
	text-align:center;
}
.header .right_btn {
	color: #000000;
	position:absolute;
	right:10px;
}
.header .right_btn img {
	width:30px;
	height:30px;
}