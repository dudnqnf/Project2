	#myprofile .div1{
		height: 200px;
		background-image: url(../../images/bg_img.png);
		background-size: 100%;
	}
	#myprofile .div2, .div3, .div4, .div5 {
		background-color: #EEEEEE;
		height: 160px;
	}
	
	#myprofile .div1 #table_info {
		height: 100%;
		width: 100%;
	}
	#section1 {
		height: 100%;
		width: 30%;
		color: white;
	}
