	/*
		cropit css
	*/
	.image-editor{
		display:none;
		position:absolute;
		width:100%;
		height:250px;
		top:50px;
	}
	.image-editor .input_background{
		display:none;
	}
	.image-editor .zoom_background{
		width:70%;
		position:absolute;
		top:20px;
		left:15%;
	}
	.image-editor .rotate-ccw{
		position:absolute;
		top:15vw;
		right:15%;
		width:6vw;
		height:6vw;
		background-size: 6vw 6vw;
		border:0;
		background-color:transparent;
	}
	.image-editor .rotate-cw{
		position:absolute;
		top:15vw;
		right:5%;
		width:6vw;
		height:6vw;
		background-size: 6vw 6vw;
		border:0;
		background-color:transparent;
	}
	.image-editor .cancel_cropit{
		position:absolute;
		top:10px;
		right:10px;
		width:6vw;
		height:6vw;
		background-size: 6vw 6vw;
		border:0;
		background-color:transparent;
	}
	.image-editor .export{
		position:absolute;
		top:50vw;
		right:10px;
		width:18vw;
		height:6vw;
		background-size: 18vw 6vw;
		border:0;
		background-color:transparent;
	}
	.image-editor .cropit-preview {
		background-color: #f8f8f8;
		background-size: cover;
		position:absolute;
		top:0px;
	}
	.image-editor .cropit-image-loaded {
		position:absolute;
		top:0px;
	}
	.image-editor .cropit-preview-image-container {
		cursor: move;
		background-color:#cccccc;
	}
	.image-editor input[type=range]{
	    -webkit-appearance: none;
	}
	
	.image-editor input[type=range]::-webkit-slider-runnable-track {
	    width: 100%;
	    height: 1vw;
	    background: #ddd;
	    border: none;
	    border-radius: 1vw;
	}
	
	.image-editor input[type=range]::-webkit-slider-thumb {
	    -webkit-appearance: none;
	    border: none;
	    height: 5vw;
	    width: 5vw;
	    border-radius: 50%;
	    background: #008ae8;
	    margin-top: -2vw;
	}
	
	.image-editor input[type=range]:focus {
	    outline: none;
	}
	
	.image-editor input[type=range]:focus::-webkit-slider-runnable-track {
	    background: #ccc;
	}
	
	/*
		text-editor css
	*/
	.text-editor{
		position:absolute;
		top:0px;
		width:100%;
		height:100%;
		display:none;
		z-index:1000;
		background-color:#ffffff;
		overflow:auto;
	}
	.text-editor .line{
		width:100%;
		height:1.2px;
		background-color:#cccccc;
	}
	.text-editor .tedit_header{
		font-family:'NanumSquareB';
		color:#ffffff;
		font-size:15px;
		font-weight:bold;
		text-align:center;
		width:100%;
		height:50px;
		border: 1px solid;
		padding-top:14px;
		background:#008ae8;
		border-color:#008ae8; 
		border-bottom-color: #dddddd;
	}
	.text-editor .tedit_header .tedit_left_btn{
		display:inline-block;
		position:absolute;
		left:5%;
		cursor:pointer;
	}
	.text-editor .tedit_header .tedit_title{
		display:inline-block;
		cursor: default;
	}
	.text-editor .tedit_header .tedit_left_btn{
		display:inline-block;
		position:absolute;
		left:5%;
	}
	.text-editor .tedit_header .tedit_right_btn{
		display:inline-block;
		position:absolute;
		right:5%;
		cursor:pointer;
	}
	.text-editor .tedit_main .input_value{
		border:0px;
		font-size: 16px;
		vertical-align: text-top;
	}
	.text-editor .tedit_input_select_button.selected{
		border : 1px solid #008ae8;
	}
	.text-editor .tedit_input_select_button.selected .btnImage{
		border : 1px solid #008ae8;
		background-color: #008ae8;
		border-radius: 50%;
	}
	.text-editor .tedit_input_select_button{
		padding: 10px;
		font-size: 16px;
		border-bottom: 1px solid #eeeeee;
	}
	
