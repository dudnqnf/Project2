	#event_info_wrapper{
		width:100vw;
		height:43vw;
	}
	#event_header_wrapper{
		padding-top:2.6vw;
		padding-left:5vw;
		height:8.97vw;
	}
	#event_header_icon{
		color:#ffffff;
		line-height:2.78vw;
		font-size:2.78vw;
		background-color:#008ae8;
		padding-top:1.39vw;
		padding-bottom:1.39vw;
		padding-left:2.2vw;
		padding-right:2.2vw;
	}
	#event_header_text{
		line-height:5.56vw;
	}
	#swiper_wrapper{
		display:none;
		position:absolute;
		top:0px;
		left:0px;
		z-index:2000;
		width:100vw;
	}
	.swiper-container{
		width:100vw;
		height:29.58vw;
		padding-bottom:1.85vw;
	}
	.swiper-slide {
		text-align: center;
		background: #ffffff;
        width: 50vw;
		/* Center slide text vertically */
		display: -webkit-box;
		display: -ms-flexbox;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-ms-flex-pack: center;
		-webkit-justify-content: center;
		justify-content: center;
		-webkit-box-align: center;
		-ms-flex-align: center;
		-webkit-align-items: center;
		align-items: center;
	}
	.event_unit_wrapper{
		width:100%;
		height:29.63vw;
	}
	.event_unit_image_container{
		position:absolute;
		width:100%;
		height:100%;
	}
	.event_unit_image{
		width:100%;
		height:100%;
	}
	
