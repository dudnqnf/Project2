<%@ include file="./header.jsp" %>

<style>
.header{
	border: 2px solid; 
	border-color:#008AE8; 
	border-bottom-color: #008AE8; 
	padding-top: 5px;
}
#close_club #space1 {
	font-size:13px;
	display: table-cell;
	vertical-align: bottom;
	padding-bottom: 7px; 
	height:50px;
	width:450px;
	padding-left: 15px;
	color: #008ae8;
	background-color: #eeeeee;
}

#close_club .check_close_club_div .line {
	height: 60px;
}
</style>
<div class="header">
	<table>
		<tr>
			<td class="left_btn">
				<img src="../resources/images/icon_back.png" 
					onclick="location.href='../club/club_detail.do?club_code=${param.club_code}'">
			</td>
			<td class="title">
				<b>동호회 폐쇄
			</td>
			<td class="right_btn">
			</td>
		</tr>
	</table>
</div>


<div id="close_club">
