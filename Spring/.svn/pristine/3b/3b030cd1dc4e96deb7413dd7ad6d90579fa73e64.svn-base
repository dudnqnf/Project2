	@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
	@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
	@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
	@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
	@charset "UTF-8";
	
	/*
		시,구 종목별 동호회 찾기
	*/
	#club_list {
		font-family:'NanumSquareL';
		background:#eeeeee;
	}
	
	#club_list .main_top_banner {
		width:100%;
		height:75px;
	}
	
	#club_list .drop_down {
		width:100%;
		height:40px;
		font-size: 12px;
	}
	
	#club_list .drop_down ul {
		width:100%;
		background:#fff;
		float:left;
	}
	
	
	#club_list .drop_down ul li {
		float:left;
		width:25%;
		height: 40px;
		padding-top: 12px;		
		text-align:center;
	}
	
	#club_list .drop_down ul ul {
		width:25%;
		display:none;
		position:absolute;
		padding-top:10px;
	}
	
	#club_list ul li:hover ul {
		display: block;
		
	}
	
	#club_list ul li:hover ul li{
		width:100%;
	}
	
	#club_list ul ul li {
		width:100%;
		height:40px;
		padding-top:15px;
		padding:15px;
	}
	#club_list #space1 {
		background-color: #f1f1f1;
		height: 8px;
	}
	#club_list .federation{
		height:153px;
		padding:8px;
		padding-right:0px;
		background: white;
	}
	
	#club_list .federation .fed_title{
		font-family:'NanumSquareEB';
		width:100%;
		color: #4f2ca5;
		text-align:center;
		font-size:15px;
	}
	
	#club_list .federation .fed_imgs{
		white-space:nowrap;
		overflow-x:scroll;
		padding-left:10px;
		padding-right:10px;
		margin-top:8px;
	}
	
	#club_list .federation .fed_imgs::-webkit-scrollbar { 
	    display: none; 
	    clear:both;
	}
	
	#club_list .federation .fed_imgs img{
		width:100px;
		height:100px;
	}
	
	#club_list .lookingforclub {
		text-align:center;
		width:100%;
		height:50px;
		padding-top:15px;
		margin-top:15px;
		background:#008ae8;
		color:#ffffff;
	}
	
	#club_list .content{
		background-image:url("../images/blank_box.png");
		background-size:100% 100%;
		height:240px;
	}
	
	#club_list .content .content_img{
		width:100%;
		height:180px;
		margin-top:10px;
	}
	
	#club_list .content_left{
		width:85%;
		height:60px;
		padding:10px;
		padding-left:13px;
		float:left;
		background:#ffffff;
	}
	
	#club_list .content_left .club_name{
		font-size:18px;
		font-family:'NanumSquareEB';
	}
	
	#club_list .content_left .club_detail{
		font-size:10px;
		font-family:'NanumSquareB';
	}
	
	#club_list .content_right{
		width:15%;
		height:60px;
		float:left;
		background:#ffffff;
	}
	
	#club_list .content_right .logo_img{
		width:35px;
		height:35px;
		margin-top:8px;
	}