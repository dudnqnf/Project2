	@charset utf-8;
	
	/*reset*/
	body, p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend, input, textarea, button, select {margin:0; padding:0;}
	img, fieldset{border:0;}

	a{color:black;}
	a:hover{text-decoration:underline;}
	
	html{

	}

	body{

	}
	div {
		border: 0px solid gray;
	}
	table, td, tr{
		border: 0px solid black;
		width:100%;
		text-align: center;
	}
	
	.footer .tab_myprofile{
		background-image:url("../../images/tab_myprofile_blue.png");
	}
	
	#space1, #space2, #space3, #space4 {
	display: table-cell;
	vertical-align: bottom;
	}
	
	#space5{
	display: table-cell;
	vertical-align: bottom;
	height:50px; width:100%; padding-bottom: 7px; color:#8C8C8C; font-size: 13px;
	}
	
	.td_uploadImage {
		border:10px solid; 
		border-color:#EEEEEE; 
		width:50%; 
		height:150px; 
		background-color: #CCCCCC
	}
	
	.header{
		font-family:'NanumSquareR';
		font-weight:bold;
		color:#008ae8;
		font-size:18px;
		text-align:center;
		width:100%;
		height:50px;
		border: 2px solid;
		background:#fff;
		padding-top:10px;
		border-color:#ffffff; 
		border-bottom-color: #D3D3D3;
	}
	.header .left_btn {
		width:15%;
	}
	.header .left_btn img {
		width:28px; 
		height: 32px; 
		padding-bottom: 4px;
	}
	.header .title {
		width:70%;
	}
	.header .right_btn {
		width:15%;
		font-size: 18px; 
		color: #8C8C8C;
	}
	.header .right_btn img {
		width:28px; 
		height: 32px; 
		padding-bottom: 4px;
	}
	<!--
		myprofile TAP > index.jsp
