<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sportsfactory.sportforall.dao.VersionDao">
	
	<select id="versionCheck" resultType="VersionDto">
		SELECT *
		FROM VERSION
		ORDER BY DATE DESC
		LIMIT 1
	</select>
	
</mapper>
