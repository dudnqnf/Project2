@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
@charset utf-8;

.header{
	font-family:'NanumSquareB';
	color:#008ae8;
	font-size:15px;
	width:100%;
	border: none;
	background:#ffffff;
}
.header .line {
	width: 100%;
	height: 1.2px;
	background-color: #dddddd;
}

.header .title {
	width:100%;
	height:100%;
	padding-top:14px;
	color:#008ae8;
	background:#ffffff;
	padding-bottom:10px;
	text-align:center;
	line-height:30px;
	cursor: default;
}

.header .title_img {
	width:100%;
	height:100%;
	padding-top:12px;
	padding-bottom:9px;
	text-align:center;
	cursor: default;
}

.header .title_img img {
	height:30px;
	margin-top:3px;
}

.header .left_btn {
	position:absolute;
	left:5%;
	top:13px;
	cursor:pointer;
}
.header .left_btn img {
	width:30px;
	height:30px;
}

.header .left_btn span {
	display:block;
	width:100px;
	height:30px;
	padding-top:5px;
	color:#000000;
}

.header .right_btn {
	position:absolute;
	right:5%;
	top:13px;
	cursor:pointer;
}
.header .right_btn img {
	width:30px;
	height:30px;
}
.header .right_btn span {
	display:block;
	padding-top:5px;
	color:#000000;
}

.header .right_sub_btn{
	position: absolute;
	right: 17%;
	top:13px;
}
.header .right_sub_btn img{
	width:28px;
	height:28px;
}

.header .dropdownList {
	position: absolute;
	width: 90px;
	right:0%;
	background-color: white;
	border: 1.2px solid #dddddd;
	font-size: 13px;
	z-index: 2;
}
.header .dropdownButton {
	padding: 13px;
	background-color: white;
}