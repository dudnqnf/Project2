	@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
	@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
	@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
	@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
	@charset utf-8;
	
	/*reset*/
	body, p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend, input, textarea, button, select {margin:0; padding:0;}
	img, fieldset{border:0;}
	
	button{
		margin:0px;
		padding:0px;
	}
	
	ul{
		list-style:none;
		padding-left:0px;
	}

	a{color:black;}
	a:hover{text-decoration:none;}
	
	html{
		background-color:#eeeeee;
	}

	body{
		width:100%;
		overflow-x:hidden;
		margin:0px;
		padding:0px;
	}
	
	body > div{
		background-color:#ffffff;
		margin-bottom:16px;
	}

	table{
		width:100%;
	}
	
	.footer .tab_home1{
		background-image:url("../images/tab_home_blue1.png");
	}
	
	/*search.jsp*/
	#search .search_tb .search_tr {
		height:50px;
	}

	#search .search_div table.search_tb {
		border: 1px;
		border-color: black;
		border-bottom-color: #cccccc;
	}
	#search .search_div .search_td {
		width:18%;
		padding-right: 15px;
		vertical-align: middle;
	}
	#search .search_div .search_number {
		height:30px; 
		width:30px; 
		border-radius: 50%; 
		background-color: #008ae8;
		padding-top : 4px;
		color : white;
		text-align: center;
		font-size:15px;
	}
	#search .search_tb .field_td {
		width:82%;
	}
	
	.header{
		margin-bottom:-1px;
	}
	#alarm_count_container{
		position:relative;
		display:table;
		top:-30px;
		left:20px;
		line-height:14px;
		-moz-border-radius:15px;
		-webkit-border-radius:15px;
		-khtml-border-radius:15px;
		border-radius:15px;
		padding-top:3px;
		padding-down:3px;
		padding-left:5px;
		padding-right:5px;
		background-color:#ff0000;
		margin-left:-5px;
	}
	#alarm_count{
		display:block;
		font-size:12px;
		color:#ffffff;
	}
	.black_blind{
		background-color:#000000;
	}
	.blue_blind{
		background-color:#4f2ca5;
	}
	.line{
		background-color:#dddddd;
		width:100%;
	}
	.footer{
		margin-bottom:0px;
		z-index:100;
	}
	#tail{
		height:60px;
		margin-bottom:0px;
	}

