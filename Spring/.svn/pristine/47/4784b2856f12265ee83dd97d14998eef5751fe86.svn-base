	@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
	@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
	@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
	@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
	@charset utf-8;
	
	/*reset*/
	body, p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend, input, textarea, button, select {margin:0; padding:0;}
	img, fieldset{border:0;}
	
	button{
		margin:0px;
		padding:0px;
	}
	
	ul{
		list-style:none;
		padding-left:0px;
	}

	a{color:black;}
	a:hover{text-decoration:none;}
	
	html{
		background-color:#eeeeee;
	}

	body{
		overflow-x:hidden;
	}
	
	body > div{
		background-color:#ffffff;
		margin-bottom:10px;
	}

	table{
		width:100%;
	}
	
	.footer .tab_home{
		background-image:url("../images/tab_home_blue.png");
	}
	
	/*search.jsp*/
	#search .search_tb .search_tr {
		height:50px;
	}

	#search .search_div table.search_tb {
		border: 1px;
		border-color: black;
		border-bottom-color: #cccccc;
	}
	#search .search_div .search_td {
		width:18%;
		padding-right: 15px;
		vertical-align: middle;
	}
	#search .search_div .search_number {
		height:30px; 
		width:30px; 
		border-radius: 50%; 
		background-color: #008ae8;
		padding-top : 4px;
		color : white;
		text-align: center;
		font-size:15px;
	}
	#search .search_tb .field_td {
		width:82%;
	}
	
	.header{
		margin-bottom:-1px;
	}
	#alarm_count_container{
		position:relative;
		display:table;
		top:-30px;
		left:20px;
		line-height:14px;
		-moz-border-radius:15px;
		-webkit-border-radius:15px;
		-khtml-border-radius:15px;
		border-radius:15px;
		padding-top:3px;
		padding-down:3px;
		padding-left:5px;
		padding-right:5px;
		background-color:#ff0000;
	}
	#alarm_count{
		display:block;
		font-size:12px;
		color:#ffffff;
	}
	.black_blind{
		background-color:#000000;
	}
	.blue_blind{
		background-color:#4f2ca5;
	}
	.line{
		background-color:#dddddd;
		width:100%;
		height:0.25vw;
	}
	.footer{
		margin-bottom:0px;
		z-index:100;
	}
	#tail{
		height:60px;
		margin-bottom:0px;
	}

	#ad_container{
		width:100vw;
		height:17vw;
	}
	
	.club_info_wrapper{
		width:100vw;
		height:60vw;
		color:#ffffff;
	}
	.club_info_image_container{
		position:absolute;
		width:100vw;
		height:60vw;
	}
	.club_info_image{
		width:100%;
		height:100%;
	}
	.club_info_blind{
		position:absolute;
		width:100vw;
		height:60vw;
		opacity:0.4;
	}
	.club_info_icon_container{
		position:relative;
		width:100vw;
		height:6vw;
		text-align:center;
		padding-top:5vw;
	}
	.club_info_icon{
		border:1px solid #ffffff;
		font-size:2.78vw;
		padding-top:0.7vw;
		padding-bottom:0.7vw;
		padding-left:4vw;
		padding-right:4vw;
	}
	.club_info_location_container{
		position:relative;
		width:100vw;
		text-align:center;
		padding-top:1.39vw;
	}
	.club_info_location{
		line-height:3vw;
		font-size:3vw;
	}
	.club_info_phrase_container{
		position:relative;
		width:100vw;
		text-align:center;
		padding-top:1.39vw;
	}
	.club_info_phrase{
		display:block;
		width:66.67vw;
		line-height:6.48vw;
		font-size:5.1vw;
		padding-left:16.665vw;
		padding-top:4.63vw;
	}
	.club_info_title_container{
		position:relative;
		width:100vw;
		text-align:center;
		padding-top:1.39vw;
	}
	.club_info_title{
		display:block;
		width:64.8vw;
		height:9.7vw;
		line-height:9.7vw;
		font-size:3.89vw;
		margin-left:16.665vw;
		margin-top:4.63vw;
		border: 1px solid #ffffff;
	}
	
	#news_header_wrapper{
