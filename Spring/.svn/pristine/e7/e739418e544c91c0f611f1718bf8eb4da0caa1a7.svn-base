	@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
	@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
	@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
	@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
	@charset utf-8;
	
	/*reset*/
	body, p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend, input, textarea, button, select {margin:0; padding:0;}
	img, fieldset{border:0;}
	
	button{
		margin:0px;
		padding:0px;
	}
	
	ul{
		list-style:none;
		padding-left:0px;
	}

	a{color:black;}
	a:hover{text-decoration:none;}
	
	html{
		margin:0px;
		padding:0px;
	}

	body{
		margin:0px;
		padding:0px;
		overflow-x:hidden;
	}

	table{
		width:100%;
	}
	
	.header{
		color:#ffffff;
		background-color:#008ae8;
	}
	
	.header .title{
		color:#ffffff;
		background:none;
	}
	
	.line{
		background-color:#eeeeee;
		width:100%;
		height:1px;
	}
	#tail{
		height:60px;
	}
	.footer .tab_home1{
		background-image:url("../images/tab_home_blue1.png");
	}

	.content{
		padding: 4.44%;
		width: 91.12%;
		display: inline-block;
	}
	.content.readflag0{
		background-color:rgba(0,138,232,0.3);
	}
	.content.readflag1{
	}
	.content.week_top_content{
		border:0.5vw dashed #008ae8;
		width:90.12%;
	}
	.pictogram_container{
		position:absolute;
		width:13.89vw;
		height:13.89vw;
		left:4.44vw;
	}
	.pictogram{
		width: 13.89%;
		-moz-border-radius: 50%;
		-webkit-border-radius: 50%;
		-khtml-border-radius: 50%;
		border-radius: 50%;
	}
	.text_wrapper{
		width: 80%;
		float: right;
	}
	.message_container{
		line-height:4.4vw;
	}
	.message{
		font-size:3.4vw;
		letter-spacing:-0.3vw;
		word-break:;
	}
	.datetime_container{
		line-height:5vw;
		vertical-align:bottom;
	}
	.datetime{
		font-size:2.5vw;
		letter-spacing:0vw;
		font-weight:bold;
		opacity:0.54;
	}