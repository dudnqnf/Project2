	@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
	@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
	@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
	@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
	@charset "utf-8";
	
	html{
		background:#eeeeee;
	}
	
	#album{
		font-family:'NanumSquareR';
	}
	
	
	#album .btn_photo_upload{
		background-image:url("../images/btn_photo_upload.png");
		background-size:100% 100%;
		cursor: pointer;
		width:50px;
		height:50px;
		position:absolute;
		z-index:10;
	}
	
	#album .search_board{
		width:100%;
		height:50px;
		background:#008ae8;
		text-align:center;
		padding:10px;	
	}
	#album .search_input_round{
		border:0;
		outline:0;
		margin: auto;
		-moz-border-radius: 20px;
		-webkit-border-radius: 20px;
		-khtml-border-radius: 20px;
		border-radius: 20px;
		background:#0762aa;
		width:95%;
		padding:0px;
		line-height: 20px;
	}
	#album .search_input{
		border:0;
		outline:0;
		background:#0762aa;
		width:70%;
		padding:5px;
		font-size:12px;
    	color: #ffffff;
	}
	#album .search_board img {
		width: 16px;
		height: 16px;
	}
	#album .club_album{
	}
	
	#album .club_album .album_container{
		float:left;
		width:49%;
		height:270px;
		padding-top:10px;
	}
	
	#album .club_album .album_container:nth-child(2n){
		padding-left:2%;
	}
	
	#album .club_album .album_container:last-child{
		margin-bottom:10px;
	}
	
	#album .club_album .album_container .lock_container{
		width:49%;
		height:200px;
		position:absolute;
	}
	
	#album .club_album .album_container:nth-child(2n) .lock_container{
		width:47%;
	}
	
	#album .club_album .album_container .lock{
		width:30px;
		height:30px;
		position:absolute;
		right:0;
		bottom:0;
	}
	
	#album .club_album .album_container .album_img{
		width:100%;
		height:200px;
	}
	
	#album .club_album .album_container .title{
		width:100%;
		height:60px;
		background:#ffffff;
		padding:10px;
		font-size:15px;
		font-weight:bold;
	}
	
	#album #input_title {
		width:300px;
		height:200px;
		display:none;
		background-color:#FFFFFF;
		position:absolute;
		z-index:2;
	}
	
	#album #input_is_closed {
		width:300px;
		height:200px;
		display:none;
		background-color:#FFFFFF;
		position:absolute;
		z-index:2;
	}
	
	.header #zzim_btn{
		position:absolute;
		top:12.5px;
		display: inline;
		width:30px;
		height:30px;
		background-image:url('../images/icon_like_blank.png');
		background-size:100% 100%;
		right:15%;
	}
	
	.header #zzim_btn.on{
		background-image:url('../images/icon_like.png');
	}

	#album_view .is_closed{
		float: right;
		text-align:right;
		padding-right:5%;
		padding-top:10px;
		font-size: 17px;
		opacity: 0.38;
	}
	
	#album_view .docTitle{
		height:50px;
		font-size:27px;
		padding-top:10px;
		padding-left:5%;
		padding-right:5%;
		padding-bottom: 10px;
	}
	
	#album_view .writer{
		height:60px;
		margin-left:15px;
		margin-right:15px;
		border-bottom-style: solid;
	    border-bottom-width: 1px;
	    border-bottom-color: #dddddd;
	}
	
	#album_view .writer .user_img{
		width:50px;
		height:50px;
		float:left;
		border-radius: 50%;
	}
	
	#album_view .writer .user_info{
		float:left;
		margin-left:10px;
		margin-top: 5px;
	}
	
	#album_view .writer #writerName {
		font-size: 13px;
		opacity: 0.87;
	}
	#album_view .writer #writeDatetime {
		font-size: 13px;
		opacity: 0.38;
	}
	#album_view #photoTouchSlider{
		height:300px;
	}
	
	#album_view #photoTouchSlider img{
		width:100%;
		height:300px;
	}
	
	#album_view #photoTouchSlider .photo_container{
		position:relative;
		margin-top:20px;
	}
	
	#album_view .swiper-slide img{
		width:100%;
	}
	
	#album_view #grayFilter{
		-webkit-filter: grayscale(100%);
		filter: grayscale(100%);
	}
	
	#album_view .like_reply_container{
		margin-top:20px;
	}
	
	#album_view .btn_like{
		padding-top:15px;
		width:50%;
		float:left;
		height:50px;
		text-align:center;
		border-style:solid;
		border-width:1px;
		border-color:#cccccc;
		border-bottom-width:0px;
		color:#cccccc;
	}
	
	#album_view .btn_like img{
		height: 29px;
		width: 21px;
		padding-bottom: 8px;
	}
	
	#album_view .likeAndCommentButton .spanInnerButton{
		font-size: 18px;
	}
	
	#album_view .btn_reply img{
		height: 29px;
		width: 21px;
		padding-bottom: 8px;
	}
	
	#album_view .btn_like.on{
		color:#008ae8;
	}
	
	#album_view .btn_reply{
		padding-top:15px;
		width:50%;
		height:50px;
		float:left;
		text-align:center;
		border-style:solid;
		border-width:1px;
		border-color:#cccccc;
		border-bottom-width:0px;
		border-left-width:0px;
		color:#cccccc;
	}
	
	#album_view .like{
		width:100%;
		height:40px;
		color:#cccccc;
		border-style:solid;
		border-width:1px;
		border-color:#cccccc;
		clear:both;
		padding:8px;
	}
	
	#album_view .reply{
		margin-bottom:40px;
	}
	
	#album_view .reply .reply_container{
		height:60px;
		padding:10px;
		border-bottom-style: solid;
	    border-bottom-width: 1px;
	    border-bottom-color: #dddddd;
	}
	
	#album_view .user_profile{
		width:40px;
		height:40px;
		margin-right:7px;
		float:left;
		-moz-border-radius:20px;
		-webkit-border-radius:20px;
		-khtml-border-radius:20px;
		border-radius:20px;
	}
	
	#album_view .reply .user_info{
		float:left;
		margin-left:10px;
	}

	#album_view .commentWriteDatetime{
		position:absolute;
		right:3%;
		font-size:12px;
		opacity:0.34;
		letter-spacing:-0.2px
	}
	
	#album_view .input_reply{
		width:100%;
		height:40px;
		position:fixed;
		bottom:0;
		padding:10px;
		border-style:solid;
		border-width:1px;
		border-color:#cccccc;
		background:#ffffff;
	}
	
	#album_view .commentContentText {
		font-size: 16px;
	}
	
	#album_view .input_reply .btn_reply_input{
		width:80%;
		border-style:none;
	}
	
	#album_view .input_reply .btn_reply_upload{
		background:#008ae8;
		color:#ffffff;
		border-style:none;
		width:15%;
		height:20px;
		font-size:13px;
	}
	
	#album_view .reply_edit{
		display:none;
		position:absolute;
		width:100px;
		background:rgba(0,0,0,0.5);
	}
	
	#album_view .reply_edit .reply_edit_btn{
		width:100%;
		padding:5px;
		text-align:center;
		color:#ffffff;
	}
	
	#album_view .reply_edit .reply_delete_btn{
		width:100%;
		padding:5px;
		text-align:center;
		color:#ffffff;
	}
	
