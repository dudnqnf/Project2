	@font-face{font-family:'NanumSquareR'; src:url('../fonts/NanumSquareR.ttf')}
	@font-face{font-family:'NanumSquareB'; src:url('../fonts/NanumSquareB.ttf')}
	@font-face{font-family:'NanumSquareL'; src:url('../fonts/NanumSquareL.ttf')}
	@font-face{font-family:'NanumSquareEB'; src:url('../fonts/NanumSquareEB.ttf')}
	@charset UTF-8;

	/*reset*/
	body, p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend, input, textarea, button, select {margin:0; padding:0;}
	img, fieldset{border:0;}
	
	ul{
		list-style:none;
		padding-left:0px;
	}

	a{text-decoration:none;}
	a:hover{text-decoration:none!important;}
	
	html{
		background:#ffffff;
	}
	#club_manager{
		font-family:'NanumSquareR';
	}
	#club_manager .total_cost{
		width:50%;
		height:100px;
		float:left;
		background:#f6f6f6;
		color:#008ae8;
		border-bottom-style: solid;
		border-right-style: solid;
		border-top-style: solid;
		border-color: rgba(155, 155, 155, 0.2);
		border-width: 1px;
		padding-top: 12px;
		padding-left: 20px;
	}
	
	#club_manager .unpaid_cost{
		width:50%;
		height:100px;
		float:left;
		background:#f6f6f6;
		border-bottom-style: solid;
		border-top-style: solid;
		border-color: rgba(155, 155, 155, 0.2);
		border-width: 1px;
		padding-top: 12px;
		padding-left: 20px;
	}
	
	#club_manager .cost_title{
		font-size:16px;
	}
	
	#club_manager .cost_value{
		font-size:35px;
	}
	
	#club_manager .member{
		width:100%;
		height:60px;
		padding:7px;
		clear:both;
	}
	
	#club_manager .member .member_image{
		width:50px;
		height:50px;
		float:left;
	}
	#club_manager .member .member_image img{
		width:50px;
		height:50px;
		margin-left: 15px;
	}
	#club_manager .member .mamber_name{
		float:left;
		padding:15px;
		margin-left: 20px;
		font-size: 15px;
	}
	
	#club_manager .member .member_checkbox{
		float:left;
		width:40px;
		height:40px;
		right:25px;
		position:absolute;
		margin-top:5px;
		background-image:url("../images/icon_admin_checkbox.png");
		background-size:100% 100%;
	}
	
	#club_manager .member .member_checkbox.checked{
		background-image:url("../images/icon_admin_checkbox_click.png");
	}
	
	#club_manager .date{
		clear:both;
		width:100%;
		height:50px;
		background:#008ae8;
		padding: 10px;
		text-align:center;
		font-size: 20px;
		color:#ffffff;
	}
	
	#club_manager .date .back{
		background-image:url("../images/icon_month_pre.png");
		background-size:100% 100%;
		width:30px;
		height:30px;
		position:absolute;
		left:30px;
	}
	
	#club_manager .date .date_fmt{
		float:left;
		width:100%;
		
	}
	
	#club_manager .date .next{
		background-image:url("../images/icon_month_post.png");
		background-size:100% 100%;
		width:30px;
		height:30px;
		float:left;
		position:absolute;
		right:30px;
	}
	
	
